<h1> Coach Sign Up </h1>

<%= form_for @coach do |f| %>

    <%= f.label :email %>
    <%= f.email_field :email, required:true %>

    <%= f.label :password %>
    <%= f.text_field :password %>

    <%= f.label :first_name %>
    <%= f.text_field :first_name %>

    <%= f.label :last_name %>
    <%= f.text_field :last_name %>

        <%= f.submit "Sign Up" %>
<% end %>
